<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
	<LinearLayout
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:orientation="horizontal"
		android:layout_alignParentRight="true"
	    android:layout_marginTop="60dp"
	    android:layout_marginRight="60dp"
	    android:gravity="center_vertical">
	    <ImageView 
	       	android:id="@+id/data_count_year_previous"
		    android:layout_width="40dp"
		    android:layout_height="40dp"
		    android:padding="5dp"
		    android:src="@drawable/previous_page"/>
	    <TextView 
	       	android:id="@+id/data_count_year_text"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:padding="5dp"
		    android:textSize="18sp"
		    android:textColor="@color/data_count_title"
		    android:text="@string/data_count_year_career"/>
	    <ImageView 
	       	android:id="@+id/data_count_year_next"
		    android:layout_width="40dp"
		    android:layout_height="40dp"
		    android:padding="5dp"
		    android:src="@drawable/next_page"/>
	</LinearLayout>
	<LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="vertical"
	    android:paddingTop="80dp"
	    android:paddingLeft="90dp"
	    android:paddingRight="70dp" >
	    <TextView 
	        android:id="@+id/data_count_title1"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:textSize="16sp"
		    android:textColor="@color/data_count_title"
		    android:text="@string/data_count_rate_total"/>
	
	    <View 
		    android:layout_width="match_parent"
		    android:layout_height="2dp"
		    android:layout_marginRight="10dp"
		    android:layout_marginTop="3dp"
		    android:background="@drawable/shape_data_count_divider"/>
	    
	    <TextView 
	        android:id="@+id/data_count_content1"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:textSize="14sp"
		    android:layout_marginTop="5dp"
		    android:textColor="@color/dark_grey"/>
	    
	    <TextView 
	        android:id="@+id/data_count_title2"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:textSize="16sp"
		    android:layout_marginTop="10dp"
		    android:textColor="@color/data_count_title"
		    android:text="@string/data_count_rate_gs"/>
	
	    <View 
		    android:layout_width="match_parent"
		    android:layout_height="2dp"
		    android:layout_marginRight="10dp"
		    android:layout_marginTop="3dp"
		    android:background="@drawable/shape_data_count_divider"/>
	    
	    <TextView 
	        android:id="@+id/data_count_content2"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:textSize="14sp"
		    android:layout_marginTop="5dp"
		    android:textColor="@color/dark_grey"/>
	    
	    <TextView 
	        android:id="@+id/data_count_title3"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:textSize="16sp"
		    android:layout_marginTop="10dp"
		    android:textColor="@color/data_count_title"
		    android:text="@string/data_count_rate_mastercup"/>
	
	    <View 
		    android:layout_width="match_parent"
		    android:layout_height="2dp"
		    android:layout_marginRight="10dp"
		    android:layout_marginTop="3dp"
		    android:background="@drawable/shape_data_count_divider"/>
	    
	    <TextView 
	        android:id="@+id/data_count_content3"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:textSize="14sp"
		    android:layout_marginTop="5dp"
		    android:textColor="@color/dark_grey"/>
	    
	    <TextView 
	        android:id="@+id/data_count_title4"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:textSize="16sp"
		    android:layout_marginTop="10dp"
		    android:textColor="@color/data_count_title"
		    android:text="@string/data_count_rate_master1000"/>
	
	    <View 
		    android:layout_width="match_parent"
		    android:layout_height="2dp"
		    android:layout_marginRight="10dp"
		    android:layout_marginTop="3dp"
		    android:background="@drawable/shape_data_count_divider"/>
	    
	    <TextView 
	        android:id="@+id/data_count_content4"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:textSize="14sp"
		    android:layout_marginTop="5dp"
		    android:textColor="@color/dark_grey"/>
	    
	    <TextView 
	        android:id="@+id/data_count_title5"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:textSize="16sp"
		    android:layout_marginTop="10dp"
		    android:textColor="@color/data_count_title"
		    android:text="@string/data_count_rate_master500"/>
	
	    <View 
		    android:layout_width="match_parent"
		    android:layout_height="2dp"
		    android:layout_marginRight="10dp"
		    android:layout_marginTop="3dp"
		    android:background="@drawable/shape_data_count_divider"/>
	    
	    <TextView 
	        android:id="@+id/data_count_content5"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:textSize="14sp"
		    android:layout_marginTop="5dp"
		    android:textColor="@color/dark_grey"/>
	    
	    <TextView 
	        android:id="@+id/data_count_title6"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:textSize="16sp"
		    android:layout_marginTop="10dp"
		    android:textColor="@color/data_count_title"
		    android:text="@string/data_count_rate_master250"/>
	
	    <View 
		    android:layout_width="match_parent"
		    android:layout_height="2dp"
		    android:layout_marginRight="10dp"
		    android:layout_marginTop="3dp"
		    android:background="@drawable/shape_data_count_divider"/>
	    
	    <TextView 
	        android:id="@+id/data_count_content6"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:textSize="14sp"
		    android:layout_marginTop="5dp"
		    android:textColor="@color/dark_grey"/>
	    
	</LinearLayout>
</RelativeLayout>