<?xml version="1.0" encoding="utf-8"?>
<com.king.mytennis.view.slidingmenu.SlidingMenuRight xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:mytennis="http://schemas.android.com/apk/res-auto"
    android:id="@+id/home_slidingmenu"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none"
    mytennis:leftPadding="@dimen/slidingmenu_rightpadding">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:gravity="center_vertical" >
        <LinearLayout 
            android:id="@+id/home_content"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="vertical"/>
       	<include layout="@layout/home_menu_layout_right"/>
    </LinearLayout>

</com.king.mytennis.view.slidingmenu.SlidingMenuRight>
